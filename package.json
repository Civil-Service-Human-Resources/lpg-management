{
	"name": "@cshr/lpg-admin",
	"version": "1.0.0",
	"description": "lpg admin application",
	"main": "server",
	"scripts": {
		"clean": "gulp setupDist",
		"sass": "sass views/assets/styles/main.scss views/assets/styles/main.css --style compressed",
		"test": "npm run test:ts",
		"test:ts": "nyc ts-mocha --paths -p ./tsconfig.json \"./test/**/*.ts\" --timeout 10000 --exit",
		"compile": "gulp typescript",
		"build": "npm run clean && npm run js && npm run sass && npm run compile",
		"dev": "nodemon src/server.ts",
		"js": "esbuild views/assets/js/main.js --bundle --minify --sourcemap --outfile=views/assets/js/main-bundle.min.js",
		"watch-js": "npm run js -- --watch",
		"watch-sass": "sass --watch views/assets/styles/main.scss views/assets/styles/main.css --style compressed",
		"start": "node dist/server.js",
		"start:build": "npm run build && node dist/server.js",
		"start:dev": "concurrently \"npm run dev\" \"npm run watch-js\" \"npm run watch-sass\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Civil-Service-Human-Resources/lpg-admin.git"
	},
	"author": "",
	"license": "ISC",
